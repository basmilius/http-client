        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/http-client.js"
        }
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "sideEffects": false,
    "dependencies": {
        "@basmilius/utils": "^1.12.0"
    },
    "devDependencies": {
        "@types/bun": "^1.2.8",
        "@types/luxon": "^3.6.0",
        "luxon": "^3.6.1",
        "tsc-alias": "^1.8.13",
        "typescript": "^5.8.2",
        "vue": "^3.5.13"
    },
    "peerDependencies": {
        "luxon": "^3.6.1",
        "vue": "^3.5.13"
    }
