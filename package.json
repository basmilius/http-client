{
    "name": "@basmilius/http-client",
    "description": "Http client for Vue projects.",
    "version": "1.4.1",
    "scripts": {
        "build": "pnpm esbuild && pnpm gentypes",
        "dev": "pnpm esbuild --watch",
        "esbuild": "esbuild src/index.ts --bundle --minify --metafile=meta.json --sourcemap --target=esnext --external:luxon --external:vue --platform=browser --format=esm --outfile=dist/basmilius.http.client.js",
        "gentypes": "tsc --noCheck && tsc-alias"
    },
    "engines": {
        "node": ">=22"
    },
    "files": [
        "dist/*"
    ],
    "type": "module",
    "main": "./dist/basmilius.http.client.js",
    "module": "./dist/basmilius.http.client.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/basmilius.http.client.js"
        }
    },
    "sideEffects": false,
    "devDependencies": {
        "@types/luxon": "^3.4.2",
        "esbuild": "^0.23.1",
        "luxon": "^3.5.0",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.6.2",
        "vue": "^3.5.4"
    },
    "peerDependencies": {
        "luxon": "^3.5.0",
        "vue": "^3.5.4"
    }
}
